<%
unless plan_groups.nil?
%>
	<li>
		<h2><%= link_to_function(h(plan_groups.name), "$('" + h(plan_groups.name) + "').toggle()") %></h2>
		<ul id="<%= h(plan_groups.name) %>" style="display: none;">
			<%= render :partial => "plans", :collection => plan_groups.plans %>
		</ul>
	</li>
<%
else
%>
	<li>No Plan Groups</li>
<%
end
%>
